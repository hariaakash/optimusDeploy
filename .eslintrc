{
    "plugins": [
        "lodash"
    ],
    "extends": [
        "airbnb",
        "plugin:lodash/recommended"
    ],
    "rules": {
        "indent": [
            2,
            4
        ],
        "func-names": 0,
        "strict": 0,
        "quote-props": 0,
        "consistent-return": 0,
        "max-len": 0,
        "lodash/prefer-map": 0,
        "lodash/import-scope": 0,
        "no-multi-assign": 2,
        "arrow-parens": [
            2,
            "as-needed"
        ],
        "class-methods-use-this": 0,
        "prefer-destructuring": 0,
        "no-bitwise": 0,
        "comma-dangle": [
            "error",
            {
                "arrays": "always-multiline",
                "objects": "always-multiline",
                "imports": "always-multiline",
                "exports": "always-multiline",
                "functions": "never"
            }
        ]
    }
}
